{

  "manifest_version": 2,
  "name": "Dropbox Paper Plus",
  "version": "1.0",

  "description": "Dropbox Paper on steroids",

  "icons": {
    "48": "icons/logo.png",
    "96": "icons/logo.png"
  },
  "permissions": [
    "https://*/" ,
    "http://*/"
  ],
  "web_accessible_resources": ["components/timer/assets/*.wav"],
  "content_scripts": [
    {
      "matches": ["*://*.paper.dropbox.com/*"],
      "js": [
        "bootstrap.js",
        "style.js",
        "libs/libs.js",
        "libs/sha.js",
        "components/popup.js",
        "components/encryption.js",
        "components/grammar.js",
        "components/ui-modifications.js",
        "components/bookmarks.js",
        "components/timer/timer.js",
        "components/sidemenu.js",
        "components/shortcuts.js",
        "script.js"
      ]
    }
  ]

}
